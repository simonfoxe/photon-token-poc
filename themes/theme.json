{
  "color": {
    "text": {
      "fill": {
        "on-color": {
          "value": "$color.text.fill.on-primary",
          "type": "color",
          "description": "$color.text.fill.on-primary"
        },
        "on-primary": {
          "value": "{colors.black.white}",
          "type": "color"
        },
        "rise": {
          "value": "{colors.green}",
          "type": "color"
        },
        "down": {
          "value": "{colors.red}",
          "type": "color"
        },
        "global": {
          "default": {
            "value": "{colors.black.00}",
            "type": "color"
          },
          "weak": {
            "value": "{colors.black.02}",
            "type": "color"
          },
          "weaker": {
            "value": "{colors.black.03}",
            "type": "color"
          }
        }
      }
    },
    "icon": {
      "fill": {
        "palette-01": {
          "value": "{colors.jasper}",
          "type": "color"
        },
        "palette-02": {
          "value": "{colors.purple}",
          "type": "color"
        },
        "palette-03": {
          "value": "{colors.green}",
          "type": "color"
        },
        "palette-04": {
          "value": "{colors.red}",
          "type": "color"
        },
        "global": {
          "actived": {
            "value": "{colors.primary}",
            "type": "color"
          },
          "default": {
            "value": "$color.text.fill.global.weak",
            "type": "color"
          }
        },
        "arrow": {
          "actived": {
            "value": "$color.text.fill.global.default",
            "type": "color"
          },
          "default": {
            "value": "$color.text.fill.global.weak",
            "type": "color"
          }
        },
        "rise": {
          "value": "$color.text.fill.rise",
          "type": "color"
        },
        "down": {
          "value": "$color.text.fill.down",
          "type": "color"
        }
      },
      "bg": {
        "palette-01": {
          "value": "rgba($color.icon.fill.palette-01,.08)",
          "type": "color"
        },
        "palette-02": {
          "value": "rgba($color.icon.fill.palette-02,.08)",
          "type": "color"
        },
        "palette-03": {
          "value": "rgba($color.icon.fill.palette-03,.08)",
          "type": "color"
        },
        "rise": {
          "value": "rgba($color.icon.fill.rise,.08)",
          "type": "color"
        },
        "palette-04": {
          "value": "rgba($color.icon.fill.palette-04,.08)",
          "type": "color"
        },
        "down": {
          "value": "rgba($color.icon.fill.down,.08)",
          "type": "color"
        },
        "primary": {
          "value": "rgba({colors.primary},.08)",
          "type": "color"
        }
      }
    },
    "btn": {
      "fill": {
        "primary": {
          "default": {
            "value": "{colors.primary}",
            "type": "color"
          }
        },
        "2nd": {
          "default": {
            "value": "rgba({colors.primary},.16)",
            "type": "color"
          }
        },
        "text": {
          "default": {
            "value": "{colors.primary}",
            "type": "color"
          }
        },
        "dorp-down": {
          "value": "{colors.black.08}",
          "type": "color"
        }
      },
      "border": {
        "ghost": {
          "default": {
            "value": "{colors.primary}",
            "type": "color"
          }
        }
      }
    },
    "div": {
      "fill": {
        "bg": {
          "value": "{colors.black.10}",
          "type": "color"
        },
        "fg": {
          "value": "{colors.black.white}",
          "type": "color"
        },
        "on-fg": {
          "default": {
            "value": "{colors.black.09}",
            "type": "color"
          }
        }
      }
    },
    "separator": {
      "fill": {
        "on-bg": {
          "value": "{colors.black.03}",
          "type": "color"
        },
        "on-fg": {
          "value": "{colors.black.08}",
          "type": "color"
        }
      }
    },
    "graph": {
      "fill": {
        "primary": {
          "value": "{colors.primary}",
          "type": "color"
        },
        "unfill": {
          "value": "{colors.black.09}",
          "type": "color"
        },
        "accent": {
          "value": "{colors.jasper}",
          "type": "color"
        }
      }
    }
  },
  "gradient": {
    "graph": {
      "fill": {
        "primary": {
          "value": "{colors.gradient.primary}",
          "type": "color"
        },
        "palette-01": {
          "value": "{colors.gradient.jasper}",
          "type": "color"
        },
        "palette-02": {
          "value": "{colors.gradient.purple}",
          "type": "color"
        }
      }
    }
  },
  "gap": {
    "text-text": {
      "value": "{spacing.default} * 2",
      "type": "spacing"
    }
  }
}